{
  "manifest_version": 3,

  "name": "CsdnHack",
  "version": "0.0.2",
  "description": "show blog.csdn.net hidden content",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },

  "content_scripts": [
    {
      "js": ["show-all.js"],
      "css": ["style.css"],
      "matches": ["https://blog.csdn.net/*"]
    }
  ]
}
